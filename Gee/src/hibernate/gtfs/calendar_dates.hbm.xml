<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
   <class name="tables.CalendarDates" table="calendar_dates">
      <meta attribute="class-description">
         This class contains the stop_times detail. 
      </meta>
      <id name="hibernateId" type="integer" column="hibernate_id">
         <generator class="native"/>
      </id>
      <property name="serviceId" column="service_id" type="string"/>
      <property name="date" column="date" type="string"/>
      <property name="exceptionType" column="exception_type" type="integer"/>
   </class>
</hibernate-mapping>

