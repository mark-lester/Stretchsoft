<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="admin.Access" table="access">
      <meta attribute="class-description">
         This class contains the user detail. 
      </meta>
      <id name="hibernateId" type="int" column="hibernate_id">
         <generator class="native"/>
      </id>
      <property name="databaseName" column="database_name" type="string"/>
      <property name="userId" column="user_id" type="string"/>
      <property name="editFlag" column="edit_flag" type="integer"/>
      <property name="adminFlag" column="admin_flag" type="integer"/>
   </class>
</hibernate-mapping>

