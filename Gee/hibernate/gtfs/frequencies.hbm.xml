<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "file://localhost/home/Gee/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="tables.Frequencies" table="frequencies">
      <meta attribute="class-description">
         This class contains the frequencies detail. 
      </meta>
      <id name="hibernateId" type="integer" column="hibernate_id">
         <generator class="native"/>
      </id>
      
      <property name="tripId" column="trip_id" type="string"/>
      <property name="startTime" column="start_time" type="string"/>
      <property name="endTime" column="end_time" type="string"/>
      <property name="headwaySecs" column="headway_secs" type="integer"/>
   </class>
</hibernate-mapping>
